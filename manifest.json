{
    "name": "twitter-customized-muting",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "カスタマイズ可能なミュート機能をTwitter公式Web版に提供",
    "content_scripts": [{
        "js": ["content.js"],
        "matches": ["https://twitter.com/*"],
        "run_at": "document_end"
    }],
    "sandbox": {
        "pages": ["sandbox.html"]
    },
    "web_accessible_resources": ["sandbox.html"]
}